{
  "name": "next-no-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn modfed:build:entry && echo \"BOOTSTRAP=$(node env.js)\" >> .env && next build",
    "lint:fix": "prettier '**/*.{ts,tsx,js,mdx}' --write",
    "modfed:build:entry": "webpack --json './public/modfed-entry.json'",
    "modfed:watch": "NODE_ENV=development webpack serve --host 0.0.0.0",
    "next:build": "next build",
    "start": "next start",
    "export": "next export",
    "clean": "rm -rf .next",
    "test": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "author": "shane osboune",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.0.7",
    "@types/react": "^17.0.2",
    "@wry/equality": "^0.4.0",
    "concurrently": "^6.0.2",
    "esbuild-loader": "^2.12.0",
    "gsap": "^3.6.1",
    "lit": "^2.0.0-rc.1",
    "next": "^10.1.3",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rehype-highlight": "^4.1.0",
    "sync-rpc": "^1.3.6",
    "tiny-invariant": "^1.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "webpack": "5.35.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "highlight.js": "^10.7.2",
    "postcss": "^8.2.12",
    "tailwindcss": "^2.1.2"
  }
}
